<mapper namespace="com.care.stay.admin.AdminStayMapper">

	<insert id="registerProc" parameterType="com.care.stay.motel.MotelDTO">
		INSERT INTO motel (mcode, mname, mregion, mdetailregion, maddress, mimage, minfo) 
